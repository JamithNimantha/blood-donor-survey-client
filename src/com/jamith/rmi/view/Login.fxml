<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="252.0" prefWidth="403.0"
            xmlns="http://javafx.com/javafx/8.0.171" fx:controller="com.jamith.rmi.controller.LoginController">
    <children>
        <Label layoutX="81.0" layoutY="59.0" prefHeight="26.0" prefWidth="62.0" text="Email :">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </Label>
        <TextField fx:id="txtEmail" layoutX="159.0" layoutY="59.0" stylesheets="@style.css"/>
        <Label layoutX="61.0" layoutY="94.0" prefHeight="26.0" prefWidth="80.0" text="Password :">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </Label>
        <PasswordField fx:id="txtPassword" layoutX="159.0" layoutY="94.0" stylesheets="@style.css"/>
        <Button fx:id="btnLogin" layoutX="278.0" layoutY="144.0" mnemonicParsing="false" onAction="#btnLoginOnAction"
                prefHeight="29.0" prefWidth="80.0" styleClass="button-save" stylesheets="@style.css" text="Login">
            <font>
                <Font name="System Bold" size="13.0"/>
            </font>
        </Button>
        <Label fx:id="lblRegister" layoutX="154.0" layoutY="198.0" onMouseClicked="#lblRegisterOnMouseClicked"
               prefHeight="17.0" prefWidth="80.0" text="Start Survey" textAlignment="CENTER" textFill="#2300d6"
               underline="true" wrapText="true">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
