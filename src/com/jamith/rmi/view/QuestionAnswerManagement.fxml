<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="621.0" prefWidth="672.0"
            xmlns="http://javafx.com/javafx/8.0.171"
            fx:controller="com.jamith.rmi.controller.QuestionAnswerManagementController">
    <children>
        <Button fx:id="btnHome" layoutX="570.0" layoutY="31.0" mnemonicParsing="false" onAction="#btnHomeOnAction"
                prefHeight="25.0" prefWidth="66.0" text="Home">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </Button>
        <TableView fx:id="tblAnswer" layoutX="96.0" layoutY="169.0" prefHeight="154.0" prefWidth="381.0">
            <columns>
                <TableColumn prefWidth="283.0" text="Answer"/>
                <TableColumn minWidth="0.0" prefWidth="50.0" style="-fx-alignment: center;"/>
                <TableColumn minWidth="0.0" prefWidth="47.0" style="-fx-alignment: center;"/>
            </columns>
        </TableView>
        <TableView fx:id="tblQuestion" layoutX="11.0" layoutY="337.0" prefHeight="276.0" prefWidth="650.0">
            <columns>
                <TableColumn prefWidth="43.0" text="ID"/>
                <TableColumn prefWidth="483.0" text="Question"/>
                <TableColumn prefWidth="60.0" style="-fx-alignment: center;"/>
                <TableColumn prefWidth="63.0" style="-fx-alignment: center;"/>
            </columns>
        </TableView>
        <Label layoutX="132.0" layoutY="39.0" prefHeight="32.0" prefWidth="391.0" text="Question And Answer Management">
            <font>
                <Font name="System Bold" size="23.0"/>
            </font>
        </Label>
        <Label layoutX="13.0" layoutY="95.0" prefHeight="31.0" prefWidth="96.0" text="Question :">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </Label>
        <TextField fx:id="txtQuestion" layoutX="96.0" layoutY="98.0" prefHeight="25.0" prefWidth="518.0"/>
        <Button fx:id="btnClear" layoutX="499.0" layoutY="288.0" mnemonicParsing="false" onAction="#btnClearOnAction"
                prefHeight="25.0" prefWidth="59.0" text="Clear"/>
        <Button fx:id="btnSave" layoutX="580.0" layoutY="288.0" mnemonicParsing="false" onAction="#btnSaveOnAction"
                prefHeight="25.0" prefWidth="59.0" text="Save"/>
        <Label layoutX="13.0" layoutY="169.0" prefHeight="31.0" prefWidth="96.0" text="Answers :">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </Label>
        <Label layoutX="13.0" layoutY="128.0" prefHeight="31.0" prefWidth="96.0" text="Answer :">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </Label>
        <TextField fx:id="txtAnswer" layoutX="96.0" layoutY="132.0" prefHeight="25.0" prefWidth="235.0"/>
        <Button fx:id="btnAdd" layoutX="359.0" layoutY="132.0" mnemonicParsing="false" onAction="#btnAddOnAction"
                prefHeight="25.0" prefWidth="59.0" text="Add"/>
        <Button fx:id="btnAnswerClear" layoutX="427.0" layoutY="132.0" mnemonicParsing="false"
                onAction="#btnAnswerClearOnAction" prefHeight="25.0" prefWidth="59.0" text="Clear"/>
    </children>
</AnchorPane>
