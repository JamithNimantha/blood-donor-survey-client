<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="645.0" prefWidth="834.0"
            xmlns="http://javafx.com/javafx/8.0.171"
            fx:controller="com.jamith.rmi.controller.AnalysisDashboardController">
    <children>
        <Label layoutX="333.0" layoutY="23.0" prefHeight="27.0" prefWidth="191.0" text="Analytical Dashboard">
            <font>
                <Font name="System Bold" size="18.0"/>
            </font>
        </Label>
        <ComboBox fx:id="cmbQuestion" layoutX="97.0" layoutY="71.0" onAction="#cmbOnAction" prefHeight="25.0"
                  prefWidth="663.0" promptText="Select a Question" styleClass="text-field" stylesheets="@style.css"
                  visibleRowCount="20"/>
        <ImageView fx:id="imageView" fitHeight="487.0" fitWidth="757.0" layoutX="40.0" layoutY="127.0"
                   pickOnBounds="true" preserveRatio="true"/>
    </children>
</AnchorPane>
